	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"update:sika": "npm i @sikaeducation/ui@latest && npm i @sikaeducation/eslint-config@latest && npm i @sikaeducation/stylelint-config@latest",
		"eject": "react-scripts eject",
		"lint": "npm run lint:ts ; npm run lint:scss ; prettier . --write",
		"lint:ts": "eslint --fix .",
		"lint:scss": "stylelint **/*.scss --fix",
		"test": "npm run test:unit && npm run test:features",
		"test:ci": "./ci-test.sh",
		"test:unit": "vitest --run --dir src",
		"test:unit:watch": "vitest --watch --dir src",
		"test:features": "npx playwright test features/",
		"test:features:watch": "nodemon -L -x 'npm run test:features' -e ts"
	},
