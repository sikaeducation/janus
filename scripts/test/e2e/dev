echo "Running Docker Compose with this configuration:"
docker-compose -f .docker/docker-compose.yml -f .docker/docker-compose.e2e.base.yml -f .docker/docker-compose.e2e.dev.yml config
docker-compose -f .docker/docker-compose.yml -f .docker/docker-compose.e2e.base.yml -f .docker/docker-compose.e2e.dev.yml -p e2e up --abort-on-container-exit
